#include <stdio.h>

int main() {
    int a, b, c;
    scanf("%d %d %d", &a, &b, &c);
    if((a>b && c>a)||(a<b && c<a))
    {
        printf("%d", a);
    }

    else if((c>b && b>a)||(c<b && b<a))
    {
        printf("%d", b);
    }

    else if((c>a && b>c)||(c<a && b<c))
    {
        printf("%d", c);
    }
    else if(a==b)
    {
        printf("%d", b);
    }
    else if(b==c)
    {
        printf("%d", c);
    }
    else if(c==a)
    {
        printf("%d", a);
    }

    return 0;
}
